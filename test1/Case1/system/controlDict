FoamFile
{
    version 2.0;
    format  ascii;
    class   dictionary;
    object  controlDict;
}

application     coatingPhaseFieldFoam;
startFrom       startTime;
startTime       0;
stopAt          endTime;
endTime         0.1;

deltaT          5e-6; //1e-4

writeControl    runTime;  
writeInterval   1e-2;

writeFormat      ascii;
//writeFormat     binary;
writeCompression off;
purgeWrite      0;