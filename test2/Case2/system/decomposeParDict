FoamFile
{
    format          ascii;
    class           dictionary;
    object          decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 16;

decomposer      hierarchical;

distributor     ptscotch;

hierarchicalCoeffs
{
    n     (8 2 1);   // x:8, y:2, z:1 (不要切 z)
    delta 0.001;
    order xyz;
}

distributed no;
roots ();

